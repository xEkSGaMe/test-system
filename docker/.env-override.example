# docker/.env-override.example
# Переопределения для продакшн

NODE_ENV=production
DEBUG=false

# Безопасные пароли для продакшн
POSTGRES_PASSWORD=strong_password_here_change_me
MONGO_PASSWORD=strong_password_here_change_me
REDIS_PASSWORD=strong_password_here_change_me

# Реальные OAuth ключи
GITHUB_CLIENT_ID=real_github_client_id
GITHUB_CLIENT_SECRET=real_github_client_secret
YANDEX_CLIENT_ID=real_yandex_client_id
YANDEX_CLIENT_SECRET=real_yandex_client_secret

# Реальный домен
WEB_CLIENT_URL=https://your-domain.com
AUTH_SERVICE_URL=https://your-domain.com/api/auth
CORE_SERVICE_URL=https://your-domain.com/api/core